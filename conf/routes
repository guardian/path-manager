# Api

GET     /paths/:id                          controllers.PathManagerController.getPathsById(id: Long)

POST    /registerNewPath                    controllers.PathManagerController.registerNewPath
POST    /registerPath                       controllers.PathManagerController.registerPath
POST    /updateCanonicalPath                controllers.PathManagerController.updateCanonicalPath
GET     /getPathDetails                     controllers.PathManagerController.getPathDetails(path: String)

GET     /debug                              controllers.PathManagerController.showDebug
GET     /updateIdSeq                        controllers.PathManagerController.showIdSeq
POST    /updateIdSeq                        controllers.PathManagerController.updateIdSeq

# Management
GET     /management/healthcheck             controllers.Management.healthCheck